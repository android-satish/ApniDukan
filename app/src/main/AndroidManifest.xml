<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developer.satish.apnidukan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/apnidukan_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/apnidukan_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ApniDukan">
        <activity android:name=".Activity.RegistrationActivity"></activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.SettingsActivity" />
        <activity android:name=".Activity.ProfileActivity" />
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>